#VIM-like keymaps for MOC

# Menu and interface control keys
go							+= l
menu_down				+= j
menu_up					+= k
menu_page_down	+= ^f
menu_page_up		+= ^b
menu_first_item	+= g
menu_last_item	+= G
search_menu			+= f
toggle_menu			+= ^h ^l
toggle_layout		+= L
help						+= H

# Directory navigation
go_to_playing_file	+= .
go_up						+= h

# Playlist specific keys
plist_move_up		= K
plist_move_down	= J
save_playlist		= S
